{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "properties": {
        "title": {
            "type": "string",
            "minLength": 1
        },
        "salary": {
            "type": "integer"
        },
        "equity": {
            "type": "number"
        },
        "companyHandle": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "title",
        "companyHandle"
    ]
}